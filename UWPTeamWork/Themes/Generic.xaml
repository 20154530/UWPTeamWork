<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:UWPTeamWork">

    <!--StaticResources-->
    <local:MinAngConverter x:Key="MinAngConverter"/>
    <local:MinAngConverterSim x:Key="MinAngConverterSim"/>
    <local:MutexVisConverter x:Key="MutexVisConverter"/>
    <local:TimeFormatdConverter x:Key="TimeFormat"/>

    <local:ChangeResCommand x:Key="ChangeRes"/>
    <local:ShowAdditionsCommand x:Key="ShowAdditionsCommand"/>
    <local:NavigateToClockCommand x:Key="NavigateToClockCommand"/>
    <local:ShowPageCommand x:Key="ShowPage"/>
    <!--Styles-->
    <!--SliderClock-->
    <Style TargetType="local:SlideClock" >
        <Setter Property="IsTabStop" Value="False"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="local:SlideClock">
                    <Grid x:Name="RootGrid" UseLayoutRounding="True">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="WindowState">
                                <VisualState x:Name="Large">
                                    <VisualState.StateTriggers>
                                        <local:WindowSizeOverTrigger MinWidowWidth="500" MinWindowHeight="500"/>
                                    </VisualState.StateTriggers>
                                    <VisualState.Storyboard>
                                        <Storyboard>
                                            <DoubleAnimation Storyboard.TargetName="ContentLayerLarge" Storyboard.TargetProperty="Opacity"
                                                             To="1" Duration="0:0:1" />
                                            <DoubleAnimation Storyboard.TargetName="ContentLayerMini" Storyboard.TargetProperty="Opacity"
                                                             From="1" To="0" Duration="0:0:1" />
                                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="ContentLayerLarge" Storyboard.TargetProperty="Visibility">
                                                <DiscreteObjectKeyFrame KeyTime="0" Value="Visible"/>
                                            </ObjectAnimationUsingKeyFrames>
                                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="ContentLayerMini" Storyboard.TargetProperty="Visibility">
                                                <DiscreteObjectKeyFrame KeyTime="0:0:1" Value="Collapsed"/>
                                            </ObjectAnimationUsingKeyFrames>
                                        </Storyboard>
                                    </VisualState.Storyboard>
                                </VisualState>
                                <VisualState x:Name="Mini">
                                    <VisualState.StateTriggers>
                                        <local:WindowSizeLessTrigger MinWidowWidth="500" MinWindowHeight="500"/>
                                    </VisualState.StateTriggers>
                                    <VisualState.Storyboard>
                                        <Storyboard>
                                            <DoubleAnimation Storyboard.TargetName="ContentLayerLarge" Storyboard.TargetProperty="Opacity"
                                                             To="0" Duration="0:0:1" />
                                            <DoubleAnimation Storyboard.TargetName="ContentLayerMini" Storyboard.TargetProperty="Opacity"
                                                             From="0" To="1" Duration="0:0:1" />
                                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="ContentLayerLarge" Storyboard.TargetProperty="Visibility">
                                                <DiscreteObjectKeyFrame KeyTime="0:0:1" Value="Collapsed"/>
                                            </ObjectAnimationUsingKeyFrames>
                                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="ContentLayerMini" Storyboard.TargetProperty="Visibility">
                                                <DiscreteObjectKeyFrame KeyTime="0" Value="Visible"/>
                                            </ObjectAnimationUsingKeyFrames>
                                        </Storyboard>
                                    </VisualState.Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                            <VisualStateGroup x:Name="SecHState">
                                <VisualState x:Name="SecToO">
                                    <Storyboard>
                                        <DoubleAnimationUsingKeyFrames Storyboard.TargetName="SecH_RotateTransform" Storyboard.TargetProperty="Angle">
                                            <EasingDoubleKeyFrame KeyTime="0:0:1" Value="0">
                                                <EasingDoubleKeyFrame.EasingFunction>
                                                    <BounceEase EasingMode="EaseOut" />
                                                </EasingDoubleKeyFrame.EasingFunction>
                                            </EasingDoubleKeyFrame>
                                        </DoubleAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="SecNormal"/>
                            </VisualStateGroup>
                            <VisualStateGroup x:Name="HandleState">
                                <VisualState x:Name="Handle_Hide">
                                    <VisualState.StateTriggers>
                                        <local:BoolTrigger BindValue="{Binding RelativeSource={RelativeSource Mode=TemplatedParent},Path=IsTimerRuning}" ReferenceValue="False"/>
                                    </VisualState.StateTriggers>
                                    <Storyboard x:Name="Handle_Hide_Ani">
                                        <DoubleAnimationUsingKeyFrames Storyboard.TargetName="HandleBG" Storyboard.TargetProperty="Opacity">
                                            <EasingDoubleKeyFrame KeyTime="0:0:1" Value="0">
                                                <EasingDoubleKeyFrame.EasingFunction>
                                                    <SineEase EasingMode="EaseIn"/>
                                                </EasingDoubleKeyFrame.EasingFunction>
                                            </EasingDoubleKeyFrame>
                                        </DoubleAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="Handle_Show">
                                    <VisualState.StateTriggers>
                                        <local:BoolTrigger BindValue="{Binding RelativeSource={RelativeSource Mode=TemplatedParent},Path=IsTimerRuning}" ReferenceValue="True"/>
                                    </VisualState.StateTriggers>
                                    <Storyboard x:Name="Handle_Show_Ani">
                                        <DoubleAnimationUsingKeyFrames Storyboard.TargetName="HandleBG" Storyboard.TargetProperty="Opacity">
                                            <EasingDoubleKeyFrame KeyTime="0:0:1" Value="1">
                                                <EasingDoubleKeyFrame.EasingFunction>
                                                    <SineEase EasingMode="EaseIn"/>
                                                </EasingDoubleKeyFrame.EasingFunction>
                                            </EasingDoubleKeyFrame>
                                        </DoubleAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                            <VisualStateGroup x:Name="TickTextState">
                                <VisualState>
                                    <VisualState.StateTriggers>
                                        <local:TickTextTrigger Angel="{Binding SecondsAng, RelativeSource={RelativeSource Mode=TemplatedParent},Mode=OneWay}"/>
                                    </VisualState.StateTriggers>
                                    <Storyboard>
                                        <DoubleAnimationUsingKeyFrames Storyboard.TargetName="Tick_Zero_ScaleTransform" Storyboard.TargetProperty="ScaleX" >
                                            <EasingDoubleKeyFrame KeyTime="0:0:1" Value="1.8">
                                                <EasingDoubleKeyFrame.EasingFunction>
                                                    <SineEase EasingMode="EaseIn"/>
                                                </EasingDoubleKeyFrame.EasingFunction>
                                            </EasingDoubleKeyFrame>
                                            <EasingDoubleKeyFrame KeyTime="0:0:3" Value="1">
                                                <EasingDoubleKeyFrame.EasingFunction>
                                                    <SineEase EasingMode="EaseIn"/>
                                                </EasingDoubleKeyFrame.EasingFunction>
                                            </EasingDoubleKeyFrame>
                                        </DoubleAnimationUsingKeyFrames>
                                        <DoubleAnimationUsingKeyFrames Storyboard.TargetName="Tick_Zero_ScaleTransform" Storyboard.TargetProperty="ScaleY" >
                                            <EasingDoubleKeyFrame KeyTime="0:0:1" Value="1.5">
                                                <EasingDoubleKeyFrame.EasingFunction>
                                                    <SineEase EasingMode="EaseIn"/>
                                                </EasingDoubleKeyFrame.EasingFunction>
                                            </EasingDoubleKeyFrame>
                                            <EasingDoubleKeyFrame KeyTime="0:0:3" Value="1">
                                                <EasingDoubleKeyFrame.EasingFunction>
                                                    <SineEase EasingMode="EaseIn"/>
                                                </EasingDoubleKeyFrame.EasingFunction>
                                            </EasingDoubleKeyFrame>
                                        </DoubleAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                                <VisualState>
                                    <VisualState.StateTriggers>
                                        <local:TickTextTrigger Angel="{Binding SecondsAng, RelativeSource={RelativeSource Mode=TemplatedParent},Mode=OneWay}" ActiveAngle="90" Mode="{Binding Mode,RelativeSource={RelativeSource Mode=TemplatedParent},Mode=OneWay}"/>
                                    </VisualState.StateTriggers>
                                    <Storyboard>
                                        <DoubleAnimationUsingKeyFrames Storyboard.TargetName="Tick_Quarter_ScaleTransform" Storyboard.TargetProperty="ScaleX" >
                                            <EasingDoubleKeyFrame KeyTime="0:0:1" Value="1.5">
                                                <EasingDoubleKeyFrame.EasingFunction>
                                                    <SineEase EasingMode="EaseIn"/>
                                                </EasingDoubleKeyFrame.EasingFunction>
                                            </EasingDoubleKeyFrame>
                                            <EasingDoubleKeyFrame KeyTime="0:0:3" Value="1">
                                                <EasingDoubleKeyFrame.EasingFunction>
                                                    <SineEase EasingMode="EaseIn"/>
                                                </EasingDoubleKeyFrame.EasingFunction>
                                            </EasingDoubleKeyFrame>
                                        </DoubleAnimationUsingKeyFrames>
                                        <DoubleAnimationUsingKeyFrames Storyboard.TargetName="Tick_Quarter_ScaleTransform" Storyboard.TargetProperty="ScaleY" >
                                            <EasingDoubleKeyFrame KeyTime="0:0:1" Value="1.5">
                                                <EasingDoubleKeyFrame.EasingFunction>
                                                    <SineEase EasingMode="EaseIn"/>
                                                </EasingDoubleKeyFrame.EasingFunction>
                                            </EasingDoubleKeyFrame>
                                            <EasingDoubleKeyFrame KeyTime="0:0:3" Value="1">
                                                <EasingDoubleKeyFrame.EasingFunction>
                                                    <SineEase EasingMode="EaseIn"/>
                                                </EasingDoubleKeyFrame.EasingFunction>
                                            </EasingDoubleKeyFrame>
                                        </DoubleAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                                <VisualState>
                                    <VisualState.StateTriggers>
                                        <local:TickTextTrigger Angel="{Binding SecondsAng, RelativeSource={RelativeSource Mode=TemplatedParent},Mode=OneWay}" ActiveAngle="180" Mode="{Binding Mode,RelativeSource={RelativeSource Mode=TemplatedParent},Mode=OneWay}"/>
                                    </VisualState.StateTriggers>
                                    <Storyboard>
                                        <DoubleAnimationUsingKeyFrames Storyboard.TargetName="Tick_Half_ScaleTransform" Storyboard.TargetProperty="ScaleX" >
                                            <EasingDoubleKeyFrame KeyTime="0:0:1" Value="1.5">
                                                <EasingDoubleKeyFrame.EasingFunction>
                                                    <SineEase EasingMode="EaseIn"/>
                                                </EasingDoubleKeyFrame.EasingFunction>
                                            </EasingDoubleKeyFrame>
                                            <EasingDoubleKeyFrame KeyTime="0:0:3" Value="1">
                                                <EasingDoubleKeyFrame.EasingFunction>
                                                    <SineEase EasingMode="EaseIn"/>
                                                </EasingDoubleKeyFrame.EasingFunction>
                                            </EasingDoubleKeyFrame>
                                        </DoubleAnimationUsingKeyFrames>
                                        <DoubleAnimationUsingKeyFrames Storyboard.TargetName="Tick_Half_ScaleTransform" Storyboard.TargetProperty="ScaleY" >
                                            <EasingDoubleKeyFrame KeyTime="0:0:1" Value="1.8">
                                                <EasingDoubleKeyFrame.EasingFunction>
                                                    <SineEase EasingMode="EaseIn"/>
                                                </EasingDoubleKeyFrame.EasingFunction>
                                            </EasingDoubleKeyFrame>
                                            <EasingDoubleKeyFrame KeyTime="0:0:3" Value="1">
                                                <EasingDoubleKeyFrame.EasingFunction>
                                                    <SineEase EasingMode="EaseIn"/>
                                                </EasingDoubleKeyFrame.EasingFunction>
                                            </EasingDoubleKeyFrame>
                                        </DoubleAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                                <VisualState>
                                    <VisualState.StateTriggers>
                                        <local:TickTextTrigger Angel="{Binding SecondsAng, RelativeSource={RelativeSource Mode=TemplatedParent},Mode=OneWay}" ActiveAngle="270" Mode="{Binding Mode,RelativeSource={RelativeSource Mode=TemplatedParent},Mode=OneWay}"/>
                                    </VisualState.StateTriggers>
                                    <Storyboard>
                                        <DoubleAnimationUsingKeyFrames Storyboard.TargetName="Tick_Last_ScaleTransform" Storyboard.TargetProperty="ScaleX" >
                                            <EasingDoubleKeyFrame KeyTime="0:0:1" Value="2">
                                                <EasingDoubleKeyFrame.EasingFunction>
                                                    <SineEase EasingMode="EaseIn"/>
                                                </EasingDoubleKeyFrame.EasingFunction>
                                            </EasingDoubleKeyFrame>
                                            <EasingDoubleKeyFrame KeyTime="0:0:3" Value="1">
                                                <EasingDoubleKeyFrame.EasingFunction>
                                                    <SineEase EasingMode="EaseIn"/>
                                                </EasingDoubleKeyFrame.EasingFunction>
                                            </EasingDoubleKeyFrame>
                                        </DoubleAnimationUsingKeyFrames>
                                        <DoubleAnimationUsingKeyFrames Storyboard.TargetName="Tick_Last_ScaleTransform" Storyboard.TargetProperty="ScaleY" >
                                            <EasingDoubleKeyFrame KeyTime="0:0:1" Value="2">
                                                <EasingDoubleKeyFrame.EasingFunction>
                                                    <SineEase EasingMode="EaseIn"/>
                                                </EasingDoubleKeyFrame.EasingFunction>
                                            </EasingDoubleKeyFrame>
                                            <EasingDoubleKeyFrame KeyTime="0:0:3" Value="1">
                                                <EasingDoubleKeyFrame.EasingFunction>
                                                    <SineEase EasingMode="EaseIn"/>
                                                </EasingDoubleKeyFrame.EasingFunction>
                                            </EasingDoubleKeyFrame>
                                        </DoubleAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                            <VisualStateGroup x:Name="MinHStates">
                                <VisualState x:Name="MinHNormal">
                                    <VisualState.StateTriggers>
                                        <local:BoolTrigger BindValue="{Binding RelativeSource={RelativeSource Mode=TemplatedParent},Path=MinHDieection}" ReferenceValue="True"/>
                                    </VisualState.StateTriggers>
                                    <Storyboard>
                                        <DoubleAnimationUsingKeyFrames Storyboard.TargetName="MinHActive_RotateTransform" Storyboard.TargetProperty="Angle">
                                            <EasingDoubleKeyFrame KeyTime="0:0:1" Value="0">
                                                <EasingDoubleKeyFrame.EasingFunction>
                                                    <CircleEase EasingMode="EaseIn"/>
                                                </EasingDoubleKeyFrame.EasingFunction>
                                            </EasingDoubleKeyFrame>
                                        </DoubleAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="MinHArrow">
                                    <VisualState.StateTriggers>
                                        <local:BoolTrigger BindValue="{Binding RelativeSource={RelativeSource Mode=TemplatedParent},Path=MinHDieection}" ReferenceValue="False"/>
                                    </VisualState.StateTriggers>
                                    <Storyboard>
                                        <DoubleAnimationUsingKeyFrames Storyboard.TargetName="MinHActive_RotateTransform" Storyboard.TargetProperty="Angle">
                                            <EasingDoubleKeyFrame KeyTime="0:0:1" Value="180">
                                                <EasingDoubleKeyFrame.EasingFunction>
                                                    <CircleEase EasingMode="EaseOut"/>
                                                </EasingDoubleKeyFrame.EasingFunction>
                                            </EasingDoubleKeyFrame>
                                        </DoubleAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                            <VisualStateGroup x:Name="MinHButtonStates">
                                <VisualState x:Name="MinHButton_Active">
                                    <VisualState.StateTriggers>
                                        <local:BoolTrigger BindValue="{Binding RelativeSource={RelativeSource Mode=TemplatedParent},Path=Focused}" ReferenceValue="True"/>
                                    </VisualState.StateTriggers>
                                    <Storyboard>
                                        <DoubleAnimationUsingKeyFrames Storyboard.TargetName="MinHInactive" Storyboard.TargetProperty="Opacity">
                                            <EasingDoubleKeyFrame KeyTime="0:0:0.8"  Value="0">
                                                <EasingDoubleKeyFrame.EasingFunction>
                                                    <SineEase EasingMode="EaseInOut"/>
                                                </EasingDoubleKeyFrame.EasingFunction>
                                            </EasingDoubleKeyFrame>
                                        </DoubleAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="MinHInactive" Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0:0:0.8" Value="Collapsed"/>
                                        </ObjectAnimationUsingKeyFrames>
                                        <DoubleAnimationUsingKeyFrames Storyboard.TargetName="MinHActive" Storyboard.TargetProperty="Opacity">
                                            <EasingDoubleKeyFrame KeyTime="0:0:0.8"  Value="1">
                                                <EasingDoubleKeyFrame.EasingFunction>
                                                    <SineEase EasingMode="EaseInOut"/>
                                                </EasingDoubleKeyFrame.EasingFunction>
                                            </EasingDoubleKeyFrame>
                                        </DoubleAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="MinHActive" Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Visible"/>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="MinHButton_InActive">
                                    <VisualState.StateTriggers>
                                        <local:BoolTrigger BindValue="{Binding RelativeSource={RelativeSource Mode=TemplatedParent},Path=Focused}" ReferenceValue="False"/>
                                    </VisualState.StateTriggers>
                                    <Storyboard>
                                        <DoubleAnimationUsingKeyFrames Storyboard.TargetName="MinHInactive" Storyboard.TargetProperty="Opacity">
                                            <EasingDoubleKeyFrame KeyTime="0:0:0" Value="0"/>
                                            <EasingDoubleKeyFrame KeyTime="0:0:0.8" Value="1">
                                                <EasingDoubleKeyFrame.EasingFunction>
                                                    <SineEase EasingMode="EaseInOut"/>
                                                </EasingDoubleKeyFrame.EasingFunction>
                                            </EasingDoubleKeyFrame>
                                        </DoubleAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="MinHInactive" Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Visible"/>
                                        </ObjectAnimationUsingKeyFrames>
                                        <DoubleAnimationUsingKeyFrames Storyboard.TargetName="MinHActive" Storyboard.TargetProperty="Opacity">
                                            <EasingDoubleKeyFrame KeyTime="0:0:0"  Value="1"/>
                                            <EasingDoubleKeyFrame KeyTime="0:0:0.8"  Value="0">
                                                <EasingDoubleKeyFrame.EasingFunction>
                                                    <SineEase EasingMode="EaseInOut"/>
                                                </EasingDoubleKeyFrame.EasingFunction>
                                            </EasingDoubleKeyFrame>
                                        </DoubleAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="MinHActive" Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0:0:0.8" Value="Collapsed"/>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                            <VisualStateGroup x:Name="TimerTextState">
                                <VisualState x:Name="TimerTextSimple">
                                    <VisualState.StateTriggers>
                                        <local:BoolTrigger BindValue="{Binding RelativeSource={RelativeSource Mode=TemplatedParent},Path=Focused}" ReferenceValue="True"/>
                                    </VisualState.StateTriggers>
                                    <Storyboard>
                                        <DoubleAnimation Storyboard.TargetName="SimpleTimer" Storyboard.TargetProperty="Opacity"
                                                         From="1" To="0" Duration="0:0:0.8">
                                            <DoubleAnimation.EasingFunction>
                                                <CircleEase EasingMode="EaseIn"/>
                                            </DoubleAnimation.EasingFunction>
                                        </DoubleAnimation>
                                        <DoubleAnimation Storyboard.TargetName="ComplexTimer" Storyboard.TargetProperty="Opacity"
                                                         From="0" To="1" Duration="0:0:0.8">
                                            <DoubleAnimation.EasingFunction>
                                                <CircleEase EasingMode="EaseIn"/>
                                            </DoubleAnimation.EasingFunction>
                                        </DoubleAnimation>
                                        <DoubleAnimation Storyboard.TargetName="ComplexTimerMil" Storyboard.TargetProperty="Opacity"
                                                         From="0" To="1" Duration="0:0:0.8">
                                            <DoubleAnimation.EasingFunction>
                                                <CircleEase EasingMode="EaseIn"/>
                                            </DoubleAnimation.EasingFunction>
                                        </DoubleAnimation>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="SimpleTimer" Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0:0:0.8" Value="Collapsed"/>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                                <VisualState>
                                    <VisualState.StateTriggers>
                                        <local:BoolTrigger BindValue="{Binding RelativeSource={RelativeSource Mode=TemplatedParent},Path=Focused}" ReferenceValue="False"/>
                                    </VisualState.StateTriggers>
                                    <Storyboard>
                                        <DoubleAnimation Storyboard.TargetName="SimpleTimer" Storyboard.TargetProperty="Opacity"
                                                         From="0" To="1" Duration="0:0:0.8">
                                            <DoubleAnimation.EasingFunction>
                                                <CircleEase EasingMode="EaseIn"/>
                                            </DoubleAnimation.EasingFunction>
                                        </DoubleAnimation>
                                        <DoubleAnimation Storyboard.TargetName="ComplexTimer" Storyboard.TargetProperty="Opacity"
                                                         From="1" To="0" Duration="0:0:0.8">
                                            <DoubleAnimation.EasingFunction>
                                                <CircleEase EasingMode="EaseIn"/>
                                            </DoubleAnimation.EasingFunction>
                                        </DoubleAnimation>
                                        <DoubleAnimation Storyboard.TargetName="ComplexTimerMil" Storyboard.TargetProperty="Opacity"
                                                         From="1" To="0" Duration="0:0:0.8">
                                            <DoubleAnimation.EasingFunction>
                                                <CircleEase EasingMode="EaseIn"/>
                                            </DoubleAnimation.EasingFunction>
                                        </DoubleAnimation>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="SimpleTimer" Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Visible"/>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                            <VisualStateGroup x:Name="TimerIconState">
                                <VisualState>
                                    <VisualState.StateTriggers>
                                        <local:BoolTrigger BindValue="{Binding RelativeSource={RelativeSource Mode=TemplatedParent},Path=IsTimerRuning}" ReferenceValue="True"/>
                                    </VisualState.StateTriggers>
                                    <Storyboard>
                                        <DoubleAnimationUsingKeyFrames Storyboard.TargetName="LargeTimerText" Storyboard.TargetProperty="Opacity" >
                                            <LinearDoubleKeyFrame KeyTime="0:0:1" Value="0"/>
                                            <LinearDoubleKeyFrame KeyTime="0:0:2" Value="1"/>
                                        </DoubleAnimationUsingKeyFrames>
                                        <DoubleAnimationUsingKeyFrames Storyboard.TargetName="LargeIcon" Storyboard.TargetProperty="Opacity" >
                                            <LinearDoubleKeyFrame KeyTime="0:0:1" Value="1"/>
                                            <LinearDoubleKeyFrame KeyTime="0:0:2" Value="0"/>
                                        </DoubleAnimationUsingKeyFrames>
                                        <DoubleAnimationUsingKeyFrames Storyboard.TargetName="SimpleTimer" Storyboard.TargetProperty="Opacity" >
                                            <LinearDoubleKeyFrame KeyTime="0:0:1" Value="0"/>
                                            <LinearDoubleKeyFrame KeyTime="0:0:2" Value="1"/>
                                        </DoubleAnimationUsingKeyFrames>
                                        <DoubleAnimationUsingKeyFrames Storyboard.TargetName="ComplexTimer" Storyboard.TargetProperty="Opacity" >
                                            <LinearDoubleKeyFrame KeyTime="0:0:1" Value="0"/>
                                            <LinearDoubleKeyFrame KeyTime="0:0:2" Value="1"/>
                                        </DoubleAnimationUsingKeyFrames>
                                        <DoubleAnimationUsingKeyFrames Storyboard.TargetName="MiniIcon" Storyboard.TargetProperty="Opacity" >
                                            <LinearDoubleKeyFrame KeyTime="0:0:1" Value="1"/>
                                            <LinearDoubleKeyFrame KeyTime="0:0:2" Value="0"/>
                                        </DoubleAnimationUsingKeyFrames>
                                        <DoubleAnimationUsingKeyFrames Storyboard.TargetName="LargeTimerMilText" Storyboard.TargetProperty="Opacity" >
                                            <LinearDoubleKeyFrame KeyTime="0:0:1" Value="0"/>
                                            <LinearDoubleKeyFrame KeyTime="0:0:2" Value="1"/>
                                        </DoubleAnimationUsingKeyFrames>
                                        <DoubleAnimationUsingKeyFrames Storyboard.TargetName="ComplexTimerMil" Storyboard.TargetProperty="Opacity" >
                                            <LinearDoubleKeyFrame KeyTime="0:0:1" Value="0"/>
                                            <LinearDoubleKeyFrame KeyTime="0:0:2" Value="1"/>
                                        </DoubleAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                                <VisualState>
                                    <VisualState.StateTriggers>
                                        <local:BoolTrigger BindValue="{Binding RelativeSource={RelativeSource Mode=TemplatedParent},Path=IsTimerRuning}" ReferenceValue="False"/>
                                    </VisualState.StateTriggers>
                                    <VisualState.Setters>
                                        <Setter Target="LargeIcon.Text" Value="&#xE769;"/>
                                        <Setter Target="MiniIcon.Text" Value="&#xE769;"/>
                                    </VisualState.Setters>
                                    <Storyboard>
                                        <DoubleAnimationUsingKeyFrames Storyboard.TargetName="LargeTimerText" Storyboard.TargetProperty="Opacity" >
                                            <LinearDoubleKeyFrame KeyTime="0:0:1" Value="0"/>
                                            <LinearDoubleKeyFrame KeyTime="0:0:2" Value="1"/>
                                        </DoubleAnimationUsingKeyFrames>
                                        <DoubleAnimationUsingKeyFrames Storyboard.TargetName="LargeIcon" Storyboard.TargetProperty="Opacity" >
                                            <LinearDoubleKeyFrame KeyTime="0:0:1" Value="0.8"/>
                                            <LinearDoubleKeyFrame KeyTime="0:0:2" Value="0"/>
                                        </DoubleAnimationUsingKeyFrames>
                                        <DoubleAnimationUsingKeyFrames Storyboard.TargetName="SimpleTimer" Storyboard.TargetProperty="Opacity" >
                                            <LinearDoubleKeyFrame KeyTime="0:0:1" Value="0"/>
                                            <LinearDoubleKeyFrame KeyTime="0:0:2" Value="1"/>
                                        </DoubleAnimationUsingKeyFrames>
                                        <DoubleAnimationUsingKeyFrames Storyboard.TargetName="ComplexTimer" Storyboard.TargetProperty="Opacity" >
                                            <LinearDoubleKeyFrame KeyTime="0:0:1" Value="0"/>
                                            <LinearDoubleKeyFrame KeyTime="0:0:2" Value="1"/>
                                        </DoubleAnimationUsingKeyFrames>
                                        <DoubleAnimationUsingKeyFrames Storyboard.TargetName="MiniIcon" Storyboard.TargetProperty="Opacity" >
                                            <LinearDoubleKeyFrame KeyTime="0:0:1" Value="0.8"/>
                                            <LinearDoubleKeyFrame KeyTime="0:0:2" Value="0"/>
                                        </DoubleAnimationUsingKeyFrames>
                                        <DoubleAnimationUsingKeyFrames Storyboard.TargetName="LargeTimerMilText" Storyboard.TargetProperty="Opacity" >
                                            <LinearDoubleKeyFrame KeyTime="0:0:1" Value="0"/>
                                            <LinearDoubleKeyFrame KeyTime="0:0:2" Value="1"/>
                                        </DoubleAnimationUsingKeyFrames>
                                        <DoubleAnimationUsingKeyFrames Storyboard.TargetName="ComplexTimerMil" Storyboard.TargetProperty="Opacity" >
                                            <LinearDoubleKeyFrame KeyTime="0:0:1" Value="0"/>
                                            <LinearDoubleKeyFrame KeyTime="0:0:2" Value="1"/>
                                        </DoubleAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                            <VisualStateGroup x:Name="ModeState">
                                <VisualState x:Name="SwitchtoTimer">
                                    <VisualState.StateTriggers>
                                        <local:SlideClockModeTrigger Mode="{Binding RelativeSource={RelativeSource Mode=TemplatedParent},Path=Mode,Mode=OneWay}"
                                                                     Reference="Timer"/>
                                    </VisualState.StateTriggers>
                                    <VisualState.Setters>
                                        <Setter Target="LargeTimerText.Visibility" Value="Visible"/>
                                        <Setter Target="LargeTimerMilText.Visibility" Value="Collapsed"/>
                                        <Setter Target="SecH_Mini_Grid.Visibility" Value="Collapsed"/>
                                        <Setter Target="ComplexTimer.Visibility" Value="Visible"/>
                                        <Setter Target="ComplexTimerMil.Visibility" Value="Collapsed"/>
                                    </VisualState.Setters>
                                    <Storyboard>
                                        <DoubleAnimation Storyboard.TargetName="DeskCircle1" Storyboard.TargetProperty="Angle"
                                                         From="20" To="0" Duration="0:0:3">
                                            <DoubleAnimation.EasingFunction>
                                                <CubicEase EasingMode="EaseOut"/>
                                            </DoubleAnimation.EasingFunction>
                                        </DoubleAnimation>
                                        <DoubleAnimation Storyboard.TargetName="DeskCircle2" Storyboard.TargetProperty="Angle"
                                                         From="-20" To="0" Duration="0:0:3">
                                            <DoubleAnimation.EasingFunction>
